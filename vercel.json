{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404" }
  ],
   "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "meet.caspar.cc"
        }
      ],
      "destination": "/meet/:path*"
    }
  ]
}

